; Managed by Puppet :: <%= @puppetmaster %>
; Do not modify this file directly it will be perodically overwritten
[main]
http_port=<%= @intserver['port'] %>
protected_environments=outage
secret_token=<%= @intserver['token'] %>
environment_method=<%= @intserver['method'] %>
production_environment=<%= @intserver['prodname'] %>
log_file=<%= @log['filename'] %>
log_max_size=<%= @log['maxsize'] %>
log_level=<%= @log['level'] %>
puppet_env_dir=<%= @intserver['envdir'] %>
runas=<%= @intserver['user'] %>
ssl_certfile=<%= @intserver['pemfile'] %>
daemonize=<%= @intserver['daemon'] %>
[r10k]
config=<%= @intr10k['config'] %>
[legacy]
enabled=<%= @intlegacy['enabled'] %>
path=<%= @intlegacy['path'] %>
gitpath=<%= @intlegacy['gitpath'] %>
[email]
method=<%= @intserver['emailmethod'] %>
from=<%= @intserver['emailfrom'] %>
smtpintserver=<%= @intserver['smtpintserver'] %>
[otrs]
enabled=<%= @intotrs['enabled'] %>
to=<%= @intotrs['email'] %>
[footprints]
enabled=<%= @intfootprints['enabled'] %>
project=<%= @intfootprints['project'] %>
to=<%= @intfootprints['email'] %>
close_status=<%= @intfootprints['close'] %>
